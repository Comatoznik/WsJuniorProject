<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sample.Entitys.ReeltorEntity" table="reeltor" schema="wsjunior">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="text"/>
        </property>
        <property name="surname">
            <column name="surname" sql-type="text"/>
        </property>
        <property name="othername">
            <column name="othername" sql-type="text"/>
        </property>
        <property name="per">
            <column name="Per" sql-type="int" not-null="true"/>
        </property>
        <property name="login">
            <column name="Login" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="password">
            <column name="Password" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>