<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sample.Entitys.RoomEntity" table="room" schema="wsjunior">
        <id name="idRoom">
            <column name="IdRoom" sql-type="int"/>
        </id>
        <property name="countRooms">
            <column name="CountRooms" sql-type="int" not-null="true"/>
        </property>
        <property name="height">
            <column name="Height" sql-type="int" not-null="true"/>
        </property>
        <property name="size">
            <column name="size" sql-type="int" not-null="true"/>
        </property>
        <property name="idObject">
            <column name="idObject" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>